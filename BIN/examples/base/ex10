%PROGRAM%

var x,y,i,coucoucoucou:int;
var continuer : bool;

type personne : struct
  test_int : int;
  test_char : char;
  test_string : string;
end struct;

function appel(x:int, y:int, z:char, a:string) return string
  var i:int;
    
  start {
    for i=2; !continuer; i=2+(i++) do {
      write ("coucou %% %%", x, y);
      x= 2.0;
    }
    
    return "coucou";
  };


start { 
  while(continuer) do {
    coucoucoucou = 7;
        
    do{
      appel(4, 4, 'c', "coucou");
    } while(i > 6);
  }

  x = (1 > !x) ? !true : !false;

  if(x != 2) then {
    ;
  }
  else if(!y) then {
    ;
  }
  else{
    ;
  }  
}






