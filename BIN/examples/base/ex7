 %PROGRAM%

var x,i,prem_fun,annee :int; 

proc prem_proc () 
    start{
        write("coucou");
    };

start{
  if x > i then {
    prem_proc(); /* erreur alors que pourtant tout est juste. On me dit aussi pour signaler l'erreur que prem_proc est une fonction, alors que c'est une procÃ©dure */
  } 
  else if(x) then {
    ;
  }
  else {
    switch i == 9 {
      case i==1 : {write("coucou");}
      case true : {x++; --i;}
      default : read(annee);
    }
  }
    
  x = (1 > 4) ? 3 : 2;
}

