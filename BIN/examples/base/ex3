%PROGRAM3%

proc proc_1 (arg:int, arg2:char) 
  var coucou : char;
  var x : int;
  var y : int;

  start {
    if (x == 6) then {
      x = 3;
    }
    else if (y == 4) then {
      write("Héhé");
    }
    else {
      write("Pas de bol");
    }
  };

function pipi (arg3:int, arg4:string[10]) return int 
  var coucou : char;
    
  start {
    x = 7; /* Erreur, mauvaise déclaration. */
    return 1;
  };

start{x=3;}

