%PROGRAM3%

proc proc_1 (arg:int, arg2:char) 
  var coucou : char;
  var x : int;
  var y : int;

  start {
    if (x == 6) then {
      x = 3;
    }
    else if (y == 4) then {
      write("Héhé");
    }
    else {
      write("Pas de bol");
    }
  };

function joujou (arg3:int, arg4:string[10]) return int 
  var coucou : char;
  var x : int;

  start {
    x = 7;
    return 1;
  };

var x : int;
var chaine : string[10];

start{
 x = 3;
 joujou(x, chaine);
}

