%PROGRAM%

var coucoucoucou, i, x : int;
var continuer : bool;

proc appel (arg : int) 
    start{
       arg += i; 
       write("%d", arg);
    };

start { 
  while (continuer) do {
    coucoucoucou = 7;
        
    for i=2; continuer; i++ do {
      write ("coucou");
      x= 2.0;
    }

    do {
      appel(100000);
    } while(i > 6);
    
  }
}






