%PROGRAM%

var coucoucoucou, i, x : int;
var continuer : bool;

proc appel (arg : int) 
    start{
       arg += i; 
       write("%d", arg);
    };

start { 
  continuer = true;

  while (continuer) do {
    coucoucoucou = 7;
        
    for i=2; i != 10; i++ do {
      write ("coucou");
      x= 2.0;
    }

    do {
      write("------> i vaut %%\n", i);
      appel(100000);
    } while(i < 6);
    
    continuer = false;
  }
}






