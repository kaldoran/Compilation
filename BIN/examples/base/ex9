%PROGRAM%
var continuer:bool;

var x,y,i:int;
var coucoucoucou: int;

type personne : struct
  test_int : int;
  test_char : char;
  test_string : string;
end struct;

var p : personne;

function appel(x:int, y:int, z:char, a:string) return string
  var i:int;
    
  start {
    for i=2; continuer; i=2+(i++) do {
      write ("coucou %d %s", x, y);
      x= 2.0;
    }
  };

start { 
    
  while continuer do {
    coucoucoucou = 7;
           
    do {
      p.test_string = appel(4, 4, 4, 4);
    } while(i > 6);  
  }
}

