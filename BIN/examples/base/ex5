  var i : int;
    
  start{
    while (1 > 2) do
    {
      write ("coucou les amis"); 
    }
  };

var coucou : char;
var a : type_normal;

start{
  a.test_omg.abc = 18; /* Réussi, les champ existent. */
  a = 18;              /* Erreur : Il faut accéder à un champ. */
  a.b = 3;             /* Erreur : Champ b n'existe pas. */
