% TOUS LES MEMES %

var myint : int;

type allsame : array[0 .. 50] of myint;
var all : allsame;

function allsame(all : int) return int
	function allsame(all : int) return int
		function allsame(all : int) return int
			function allsame(all : int) return int
				function allsame(all : int) return int
				start {
					return all + allsame(all) * allsame(all);
				};
			start {
				return allsame(all++);
			};
		start {
			return allsame(all++);
		};
	start {
		return allsame(all++);
	};
start {
	return allsame(all++);
};


start {
	myint = 0;

	for(;myint < 50;myint = allsame(myint)) do {
		all[myint] = myint;
	}
	;
}
